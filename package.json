{
  "name": "web-apps",
  "devDependencies": {
    "prettier": "^3.4.2",
    "syncpack": "^13.0.2",
    "turbo": "^2.3.3",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.3",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md,css,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,css,json}\"",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
