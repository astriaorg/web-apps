{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "flame-defi#build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "env": [
        "NEXT_PUBLIC_VERSION",
        "NEXT_PUBLIC_BRAND_URL",
        "NEXT_PUBLIC_BRIDGE_URL",
        "NEXT_PUBLIC_SWAP_URL",
        "NEXT_PUBLIC_POOL_URL",
        "NEXT_PUBLIC_EARN_API_URL",
        "NEXT_PUBLIC_GA_TRACKING_ID",
        "NEXT_PUBLIC_FEEDBACK_FORM_URL",
        "NEXT_PUBLIC_WALLET_CONNECT_DOMAIN_VERIFICATION_CODE",
        "NEXT_PUBLIC_NETWORK_LIST_OPTIONS",
        "NEXT_PUBLIC_ENV"
      ],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^test"]
    }
  }
}
